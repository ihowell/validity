{
    "in_dataset": "mnist",
    "out_dataset": "fmnist",
    "additional_out_datasets": [],
    "classifiers": [
        {
            "name": "base",
            "type": "mnist",
            "train_kwargs": {
                "tensorboard_path": "tensorboard/classifiers/cls_mnist_mnist_base",
                "max_epochs": 3
            }
        },
        {
            "name": "spectral_norm",
            "type": "mnist",
            "cls_kwargs": {
                "spectral_normalization": true
            },
            "train_kwargs": {
                "max_epochs": 3,
                "tensorboard_path": "tensorboard/classifiers/cls_mnist_mnist_spectral_norm"
            }
        }
    ],
    "generators": [
        {
            "type": "mnist_vae",
            "kwargs": {
                "beta": 10.0
            }
        }
    ],
    "adv_attacks": [
        "fgsm"
    ],
    "contrastive_methods": [
        "am"
    ],
    "contrastive_kwargs": {
        "shards": 1,
        "batch_size": 16
    },
    "contrastive_subset": 1
}